package application;

import java.util.Scanner;

public class Program {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int m = sc.nextInt();
		int n = sc.nextInt();
		
		int mat[][] = new int[m][n];
		
		for(int i=0; i<n; i++) {
			for(int j=0; j<n; j++) {
				mat[i][j] = sc.nextInt();
			}
		
		int x = sc.nextInt();
			 
		 for( i=0; i<n; i++) {
			 for(int j=0; j<n; j++) {
				System.out.println(x);
			}
				
		}
		
		
			sc.close();
	   }

	}
	
}
