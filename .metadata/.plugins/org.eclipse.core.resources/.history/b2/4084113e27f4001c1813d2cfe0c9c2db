package entiites;

public class Employee {
	
	private int Id;
	private String name;
	private double salary;
	private double percent;
	
	public Employee(int id, String name, double salary, double percent) {
		Id = id;
		this.name = name;
		this.salary = salary;
		this.percent = percent;
	}
	public  int getId() {
		return Id;
	}
	public void setId(int id) {
		Id = id;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public double getSalary() {
		return salary;
	}
	public void setSalary(double salary) {
		this.salary = salary;
	}

	public void salaryAdd() {
		this.salary += percent / 100.00;
	}
	
}
